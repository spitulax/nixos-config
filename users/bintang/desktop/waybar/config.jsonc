{
  "position": "top",
  "layer": "top",
  "spacing": 12,
  "height": 30,
  // "margin": "6 6",

  "modules-left": [
    "group/menu",
    "memory",
    "custom/separator",
    "cpu",
    "hyprland/window",
  ],
  "modules-center": [
    "hyprland/workspaces",
  ],
  "modules-right": [
    "network",
    "pulseaudio",
    "battery",
    "clock",
    "group/buttons",
    "custom/separator",
    "tray"
  ],
  "hyprland/workspaces": {
    "format": "{icon}",
    "show-special": true,
    "format-icons": {
      // "1": "",
      // "2": "󰊯",
      // "3": "󰖣",
      // "4": "󰝚",
      // "5": "󰓓",
      "special": "󰣕",
      "active": "⬤",
      "default": "◯",
    },
    "on-click": "activate",
    "persistent-workspaces": {
      "*": 5
    },
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
  },

  "hyprland/window": {
    "format": "{class}",
    "max-length": 160,
    "separate-outputs": true,
    "on-click": "exec hyprctl dispatch killactive",
    // This looks stupid, but it *mostly* works
    // But I still have to update this manually

    "rewrite": {
      "brave-agimnkijcaahngcdmfeangaknmldooml-Default": "YouTube",
      "brave-browser": "Brave Web Browser",
      "brave-comkdlimbkhemidbbpchhepidbmjpnhh-Default": "Twitch",
      "brave-hophjnbpmamkldmdaeggjlnpfechpkfl-Default": "Bitwarden",
      "brave-mfinobjnbcnohnemakjeccbjljpebmlm-Default": "Syncthing",
      "brave-mjoklplbddabcmpepnokjaffbmgbkkgg-Default": "GitHub",
      "brave-nhpkhfhppiampjblenncchhnipmeafgd-Default": "Anilist",
      "brave-pjibgclleladliembfgfagdaldikeohf-Default": "Spotify",
      "brave-pliiebkcmokkgndfalahlmimanmbjlab-Default": "Discord",
      "btop": "btop++",
      "com.obsproject.Studio": "OBS Studio",
      "com.rtosta.zapzap": "ZapZap",
      "fish": "fish",
      "kitty": "kitty",
      "kitty-open": "kitty URL Launcher",
      "krita_brush": "Krita",
      "krita_csv": "Krita",
      "krita_exr": "Krita",
      "krita_gif": "Krita",
      "krita_heif": "Krita",
      "krita_heightmap": "Krita",
      "krita_jp2": "Krita",
      "krita_jpeg": "Krita",
      "krita_jxl": "Krita",
      "krita_kra": "Krita",
      "krita_krz": "Krita",
      "krita_ora": "Krita",
      "krita_pdf": "Krita",
      "krita_png": "Krita",
      "krita_psd": "Krita",
      "krita_qimageio": "Krita",
      "krita_raw": "Krita",
      "krita_spriter": "Krita",
      "krita_svg": "Krita",
      "krita_tga": "Krita",
      "krita_tiff": "Krita",
      "krita_webp": "Krita",
      "krita_xcf": "Krita",
      "kvantummanager": "Kvantum Manager",
      "mpv": "mpv Media Player",
      "lutris": "Lutris",
      "nm-applet": "NetworkManager Applet",
      "nm-connection-editor": "Advanced Network Configuration",
      "nvim": "Neovim wrapper",
      "nwg-look": "GTK Settings",
      "org.gnome.Zenity": "Zenity",
      "org.godotengine.Godot4": "Godot Engine 4",
      "org.kde.dolphin": "Dolphin",
      "org.kde.krita": "Krita",
      "org.kde.polkit-kde-authentication-agent-1": "PolicyKit Authentication Agent",
      "org.https://nomacs.": "nomacs",
      "org.pwmt.zathura-cb": "Zathura",
      "org.pwmt.zathura": "Zathura",
      "org.pwmt.zathura-djvu": "Zathura",
      "org.pwmt.zathura-pdf-mupdf": "Zathura",
      "org.pwmt.zathura-ps": "Zathura",
