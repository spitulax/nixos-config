    {
      "type": "custom",
      "format": "\u001b[30m│"
    },

    {
      "type": "cpu",
      "key": "\u001b[30m│\u001b[33m ╭─  "
    },
    {
      "type": "gpu",
      "key": "\u001b[30m│\u001b[33m ├─󰍹  "
